<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="422.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="softwareii_project.Views.CustomerScreenController">
   <children>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
         <children>
            <Pane layoutX="1.0" layoutY="1.0" prefHeight="416.0" prefWidth="370.0">
               <children>
                  <TableView fx:id="customerTable" layoutX="22.0" layoutY="101.0" prefHeight="200.0" prefWidth="326.0">
                    <columns>
                      <TableColumn id="partID" fx:id="custIDColumn" prefWidth="84.0" text="ID" />
                      <TableColumn id="partName" fx:id="custNameColumn" prefWidth="241.0" text="Customer Name" />
                    </columns>
                  </TableView>
                  <Label layoutX="86.0" layoutY="31.0" text="Customer Management" textFill="#190072">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <Button id="partAddBtn" fx:id="custAddBtn" layoutX="177.0" layoutY="327.0" mnemonicParsing="false" onAction="#onCustomerAddBtnClick" text="Add" />
                  <Button id="partModBtn" fx:id="custModBtn" layoutX="231.0" layoutY="327.0" mnemonicParsing="false" onAction="#onCustomerModBtnClick" text="Modify" />
                  <Button id="partDelBtn" fx:id="custDelBtn" layoutX="297.0" layoutY="327.0" mnemonicParsing="false" onAction="#onCustomerDelBtnClick" text="Delete" />
                  <Button fx:id="cancelBtn" layoutX="296.0" layoutY="377.0" mnemonicParsing="false" onAction="#cancelBtnHandler" text="Cancel" />
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
